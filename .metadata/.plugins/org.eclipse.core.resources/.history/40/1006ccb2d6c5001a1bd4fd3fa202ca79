package runner;

import threads.HRMHomeThread;

public class TestRunner {

	public static void main(String[] args) {
		
		Thread t1=new HRMHomeThread("Thread 1");
		Thread t2=new HRMHomeThread("Thread 2");
		Thread t3=new HRMHomeThread("Thread 3");
		
		//System.out.println("Thread Started.........");
		t1.start();
		t2.start();
		t3.start();
		
	}

}
